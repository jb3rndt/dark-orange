  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "name": "Variable",
      "scope": ["string constant.other.placeholder", "entity.name.variable"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Function Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Function Argument",
      "scope": ["meta.function-call.arguments"],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control",
